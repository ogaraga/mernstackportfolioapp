{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" },
      { "src": "/Backend/app.js", "use": "@vercel/node" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/Backend/app.js" },
      { "source": "(.*)", "destination": "/index.html" }
    ]
  }